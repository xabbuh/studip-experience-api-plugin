{
    "name": "xabbuh/studip-experience-api-plugin",
    "description": "Integration of the Experience API into the Stud.IP LMS",
    "keywords": ["Experience API", "xAPI", "Stud.IP", "Stud.IP plugin", "LRS", "Learning Record Store"],
    "type": "studip-plugin",
    "license": "MIT",
    "homepage": "https://github.com/xabbuh/studip-experience-api-plugin",
    "authors": [
        {
            "name": "Christian Flothmann",
            "homepage": "https://github.com/xabbuh"
        }
    ],
    "autoload": {
        "psr-4": {
            "Xabbuh\\ExperienceApiPlugin\\Model\\": "Model",
            "Xabbuh\\ExperienceApiPlugin\\Storage\\PdoMysql\\": "PdoMysql"
        }
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "php-xapi/model": "^1.0.1",
        "php-xapi/repository-api": "~0.3",
        "php-xapi/serializer": "~1.0",
        "php-xapi/symfony-serializer": "~1.0",
        "php-xapi/validation": "~0.2",
        "ramsey/uuid": "^2.9",
        "symfony/phpunit-bridge": "^3.4 || ^4.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
